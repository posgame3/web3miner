{
  "title": "PixelMiner Documentation",
  "description": "Official documentation for PixelMiner game",
  "author": "PIXELMINER Team",
  "language": "en",
  "structure": {
    "readme": "README.md",
    "summary": "SUMMARY.md"
  },
  "plugins": [
    "theme-default",
    "highlight",
    "search",
    "sharing",
    "fontsettings",
    "livereload"
  ],
  "pluginsConfig": {
    "image-captions": {
      "caption": "Image _CAPTION_"
    },
    "theme-default": {
      "showSummary": true,
      "showLevel": true
    },
    "sharing": {
      "facebook": true,
      "twitter": true,
      "google": false,
      "weibo": false,
      "instapaper": false,
      "vk": false,
      "all": ["facebook", "twitter"]
    }
  }
} 